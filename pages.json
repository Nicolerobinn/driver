{
	"easycom": {
		 "^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue"
	},
	"subPackages": [{
		"root": "pagesA",
		"pages": [{
			"path": "promotion",
			"style": {
				"navigationBarTitleText": "推广二维码"
			}
		}, {
			"path": "personal",
			"style": {
				"navigationBarTitleText": "个人资料",
				"enablePullDownRefresh": false
			}

		}, {
			"path": "announcementList",
			"style": {
				"navigationBarTitleText": "答题记录"
			}
		},{
			"path": "tutorial",
			"style": {
				"navigationBarTitleText": "使用教程"
			}
			
		},{
			"path": "commission",
			"style": {
				"navigationBarTitleText": "佣金"
			}
		},{
			"path": "package",
			"style": {
				"navigationBarTitleText": "套餐"
			}
		}
		]
	}],
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index",
			"style": {
				"navigationStyle": "custom",
				"navigationBarTitleText": "首页"
			}
		},		{
			"path": "pages/my",
			"style": {
				"navigationBarTitleText": "我的",
				"navigationBarBackgroundColor": "#1296db",
				"navigationBarTextStyle": "white" //状态栏字体颜色
			}
		},		{
			"path": "pages/search",
			"style": {
				"navigationBarTitleText": "搜索题目"
			}
		},{
			"path": "pages/practice",
			"style": {
				"navigationBarTitleText": "练习"
			}
		}
	],
	"tabBar": {
		"color": "#333",
		"selectedColor": "#1296db",
		"borderStyle": "black",
		"backgroundColor": "#ffffff",
		"list": [{
				"pagePath": "pages/index",
				"text": "首页",
				"iconPath": "static/img/home.png",
				"selectedIconPath": "static/img/selectHome.png"
			},
			{
				"pagePath": "pages/search",
				"text": "搜索题目",
				"iconPath": "static/img/search.png",
				"selectedIconPath": "static/img/selectSearch.png"
			},
			{
				"pagePath": "pages/practice",
				"text": "练习",
				"iconPath": "static/img/practice.png",
				"selectedIconPath": "static/img/selectPractice.png"
			},
			{
				"pagePath": "pages/my",
				"text": "我的",
				"iconPath": "static/img/my.png",
				"selectedIconPath": "static/img/selectMy.png"
			}
		]
	},
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	}
